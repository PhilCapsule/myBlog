    <div class="home">
        <div class="row">
            <div class="col-12 text-center get_started" >
                <h2> Login </h2> <!-- change title -->
            </div>


                <!-- sign UP Area  -->
            <div class="col-12 col-sm-6 text-center ">
                <div class="formArea">  <!--sign-up--> 
                    <h4 class="titleform"> Sign Up</h4>

                    <form action="/sign-up" method="POST" id="gauche">

                        <div class="form-group" >
                            <label for="exampleInputUsername">Username</label>
                            <input  name="usernameFront" type="text" class="form-control" placeholder="Enter username" >
                        </div>

                        <div class="form-group">
                            <label for="exampleInputUsername">Email adress</label>
                            <input  name="emailFront" type="email" class="form-control" placeholder="Enter username" >
                        </div>

                        <div class="form-group">
                            <label for="exampleInputUsername">Password</label>
                            <input  name="passwordFront" type="password" class="form-control" placeholder="Enter username" >
                        </div>

                        <button id="btnsubmit" type="submit" class="btn btn-info">Submit</button>

                    </form>
                    <!-- /form gauche  -->
                </div>
            </div>
            <!-- /formArea  --------------------------------------------  -->

            <!-- sign in cote droit  -->
            <div class="col-12 col-sm-6 text-center">
                <div class="formArea">  <!-- sign-In -->
                    <h4 class="titleForm"> Sign In</h4>

                    <form action="/sign-in" method="POST">
                        <div class="form-group">
                            <label for="exampleInputEmail">Email Adress</label>
                            <input name="emailFront" type="email" class="form-control" aria-describedby="emailHelp" placeholder="Enter email">
                        </div>

                        <div class="form-group">
                            <label for="exampleInputPassword">Password</label>
                            <input name="passwordFront" type="password" class="form-control" aria-describedby="emailHelp" placeholder="Password">
                        </div>

                        <button id="btnsubmit2" type="submit" class="btn btn-info">Submit</button>
                    </form>
                    <!-- form droit -->

                </div> <!-- formArea droit-->
            </div> <!-- class col_12-->
        </div> <!-- home / row-->
